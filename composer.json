{
    "name": "gomicale/gomicale",
    "description": "Nishinomiya garbage calendar checker",
    "type": "script",
    "keywords": [
        "gomicale"
    ],
    "homepage": "https://github.com/radiocat/gomicale/",
    "license": "MIT",
    "authors": [
        {
            "name": "radiocat"
        }
    ],
    "support": {
        "issues": "https://github.com/radiocat/gomicale/issues"
    },
    "require": {
        "php": "^7.4.0",
        "vlucas/phpdotenv": "^4.2",
        "paquettg/php-html-parser": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Gomicale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gomicale\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit test/"
    }
}